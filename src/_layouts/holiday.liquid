<!doctype html>
<!-- saved from url=(0035)https://holidaycss.js.org/template/ -->
<html lang="{{ site.locale }}" class="holiday-css-dark">
  <head>
    {% render "head", metadata: site.metadata, title: data.title %}
    <link rel="stylesheet" href="{% asset_path css %}">
  </head>
  <body class="{{ data.layout }} {{ data.page_class }}" data-ryu-obtrusive-scrollbars="true">
    {% render "navbar", metadata: site.metadata, resource: resource %}

    <main>
      {{ content }}
    </main>

    {% render "footer", metadata: site.metadata %}
  </body>
</html>
